{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Sender (Debug)",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/Debug/ovbsend.exe",
            "preLaunchTask": "",
            "args": [
                "--file",
                "${workspaceFolder}\\resources\\ducks.y4m",
                "--codec",
                "HEVC",
                // "--encoder-config",
                // "--format=400",
                "--encoder-config",
                "--ovc-bits-per-pixel=0.455,--ovc-repeat-vps=0,--ovc-num-parts-exp=2,--ovc-num-levels=3,--ovc-wavelet-family=biorthogonal,--ovc-wavelet-config=11,--ovc-partition-type=offset-zerotree,--ovc-spiht=enable,--ovc-entropy-coder=arithmetic,--ovc-interleaver=random",
                "--encoder-config",
                "--xvc-num-ref-pics=0,--xvc-max-key-pic-distance=0,--xvc-qp=10",
                "--encoder-config",
                "--vvc-gop-size=1,--vvc-intra-period=1",
                "--encoder-config",
                "--avc-qp=29,--avc-intra-period=1,--avc-num-ref-frame=0",
                "--encoder-config",
                "--hevc-sop-structure=intra,--hevc-qp=26",
                "--ip",
                "127.0.0.1",
                "--port",
                "4444",
                "--log-level",
                "details",
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "type": "cppvsdbg",
        },
        {
            "name": "Launch Relay (Debug)",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/Debug/ovbrelay.exe",
            "preLaunchTask": "",
            "args": [
                "--recv-port",
                "4444",
                "--send-ip",
                "127.0.0.1",
                "--send-port",
                "8888",
                "--tamper-config",
                "--prob=0",
                "--drop-type",
                "simple_bursty",
                "--drop-config",
                "--hg=0,--hb=1,--p=0.00525643913794398,--q=0.144927536231884",
                "--log-level",
                "details",
                "--seed",
                "10007"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "type": "cppvsdbg",
        },
        {
            "name": "Launch Receiver (Debug)",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/Debug/ovbrecv.exe",
            "preLaunchTask": "",
            "args": [
                "--file",
                "${workspaceFolder}\\output\\unideal\\hevc\\duck\\50.y4m",
                "--codec",
                "HEVC",
                "--port",
                "8888",
                "--log-level",
                "details",
                "--decoder-config",
                "--vvc-error-handling-flags=try-continue",
                "--decoder-config",
                "--ovc-error-concealment=average-received",
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "type": "cppvsdbg",
        },
        {
            "name": "Launch Editor (Debug)",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/Debug/ovbeditor.exe",
            "preLaunchTask": "",
            "args": [
                "--original",
                "${workspaceFolder}\\resources\\carphone_qcif.y4m",
                "--distorted",
                "${workspaceFolder}\\output\\ideal\\avc\\0.25.y4m",
                "--output",
                "${workspaceFolder}\\dsis\\avc\\0.25.y4m",
                "--framerate",
                "30",
                "--method",
                "dsis",
                "--log-level",
                "details"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "type": "cppvsdbg",
        },
    ]
}